{
  "name": "ts-rust-cache",
  "author": "duasfh",
  "version": "0.1.0",
  "type": "module",
  "license": "ISC",
  "packageManager": "pnpm@10.21.0",
  "scripts": {
    "build-pre": "cd ../rs && rm -rf ./pkg && wasm-pack build --target nodejs",
    "build": "pnpm build-pre && rm -rf ./dist && tsc && cp -r ./pkg/ ./dist/pkg"
