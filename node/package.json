  "dependencies": {
    "ms": "^2.1.3"
  },
  "devDependencies": {
    "@types/ms": "^2.1.0",
    "@types/node": "^25.0.3",
    "typescript": "^5.9.3"
  },
  "keywords": [
    "rust",
    "wasm",
    "cache"
  ],
  "main": "dist/bridge.js",
  "types": "dist/bridge.d.ts",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  }
}
